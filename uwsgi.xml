<uwsgi>
  <socket>127.0.0.1:8000</socket>
  <listen>20</listen>
  <master>true</master>
  <pidfile>/srv/www/equator/webadmin/template/uwsgi.pid</pidfile>
  <processes>2</processes>
  <module>django_wsgi</module>
  <pythonpath>/srv/www/equator/webadmin/template</pythonpath>
  <profiler>true</profiler>
  <memory-report>true</memory-report>
  <enable-threads>true</enable-threads>
  <logdate>true</logdate>
  <limit-as>6048</limit-as>
  <daemonize>/srv/www/equator/webadmin/template/daily.log</daemonize>
</uwsgi>